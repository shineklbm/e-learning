/*********************** Style Sheet *****************************/
@font-face {
    font-family: 'openSans';
    src: url('../fonts/opensans-regular/opensans-regular-webfont.eot');
    src: url('../fonts/opensans-regular/opensans-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/opensans-regular/opensans-regular-webfont.woff2') format('woff2'),
         url('../fonts/opensans-regular/opensans-regular-webfont.woff') format('woff'),
         url('../fonts/opensans-regular/opensans-regular-webfont.ttf') format('truetype'),
         url('../fonts/opensans-regular/opensans-regular-webfont.svg#open_sansregular') format('svg');
    font-weight: normal;
    font-style: normal;

}
/*------------------------------- Common -----------------------------------------*/
html, body { height: 100%; }
html,body,header,footer,section,aside,div,span,label,input,article,p,h1,h2,h3,h4,h5,h6{
	margin: 0;
	padding: 0;
	font-family: 'openSans';
	outline: none;	
}
a,a:hover{ text-decoration: none; color: #000; cursor: pointer; }
.no-margin{margin: 0;}
.no-marign-right{ margin-right: 0; }
.no-margin-left{ margin-left: 0; }
.margin-bottom{ margin-bottom: 30px; }
.no-padding{padding: 0;}
.no-border{border: 0;}
.no-shadow{box-shadow: 0;}
.inline{display: inline-block;}
img{ max-width: 100%; height: auto; }
header{ background: #e3e2e2; }
h1, h2, h3,h4, h5, h6,p{ color: #3b3b3b; margin-bottom: 20px;}
h1{	font-size: 19px; text-transform: uppercase; }
h2{	font-size: 17px; }
h3{ font-size: 16px; }
p{ font-size: 14px; }
.blue-wrapper{ background: #336699; color: #fff; padding:30px; margin-bottom: 30px; opacity: .9; }
.white-wrapper{ background: #FFF; color: #fff; padding:30px; margin-bottom: 30px; opacity: .9; }
.blue-wrapper h1, .blue-wrapper p{ color: #fff;}
.close{
	color: #fff;
	font-size: 18px;
	margin-top: 0;
	float: right;
	display: block;
	cursor: pointer;
	opacity: 1;
	font-weight: normal;
	text-shadow:none;
	line-height: 1.5;
}

.close:after{
	content: '\e606';
	font-family: 'icomoon';
	
}
/*================================== COMPONENTS ==============================================*/
/*------------------ Text Component ----------*/
.text-component{
	font-size: 14px ;
	font-weight: normal;
	line-height: 2.3rem;
}
/*------------------ Image Component --------------*/
.image-component img{
	max-width: 100%;
	height: auto;
}
/*------------------- Click and Reveal Popup ------*/
.modal-backdrop { background-color: #fff; opacity:0.8; }
.modal .modal-content{ border-radius: 0; box-shadow: none; border: 0;}
.modal .modal-header{ background: #336699;	color: #fff;}
.modal .modal-header .close{color: #fff;font-size: 37px; margin-top: 0; float: right; display: block; cursor: pointer; opacity: 1; font-weight: lighter; text-shadow:none; line-height: 2.3rem;}
.modal .modal-title { color: #fff;font-weight: bold; font-size: 16px; line-height: 2.3rem}
.modal .modal-body{ background: #F1F0F0;color: #000; font-weight: normal; font-size: 14px; line-height: 2.3rem;}

/*================================== COMPONENTS ==============================================*/

/*--------------------------------- Header -------------------------------------  */
header{ padding: 6px 0; border-left: 6px #e3801b solid; }
header #menu-wrapper{  margin-left: -7px; min-height: 44px; margin-top: -6px; margin-bottom: -6px;text-align: center;  }
header #menu-wrapper span{ cursor: pointer; text-align: center; font-size: 22px; display: block; color: #00457B;margin-top: 10px;}
header h1, header h2{ display: inline-block; padding: 6px 0; margin: 0; }
header h1{ border-right: #cecdcd 1px solid; padding-right: 17px; margin-right: 26px; }
header #logo-wrapper{ text-align: right; }
header #logo-wrapper span{ padding: 6px 0; display: inline-block; margin-right: 16px; padding-right: 18px; border-right: #cecdcd 1px solid; }
/*--------------------------------- Header -------------------------------------  */

/*--------------------------- fullwidth background -----------------------------*/
#background-wrapper{
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	position: absolute;
	z-index: -1;
	overflow: hidden;
}
#background-wrapper img{
	width: 100%;
	height: auto;
}
/*---------------------------- fullwidth background ---------------------------*/

/*-------------------------------- Menu ---------------------------------------*/
#inner-wrapper{
	position: relative;
}
#menu-wrapper span{
	display: block;
}
#vertical-menu-wrapper{
	position: absolute;
	background: #4E4E4E;
	opacity: 0.9;
	color: #E1E1E1;
	display: none;
	height: auto;
	padding: 30px;
	min-width: 315px;
	z-index: 1000;
	top: -30px;
}
#vertical-menu-wrapper h3{
	color: #fff;
	font-size: 22px;
	margin-top: 0;
	display: inline-block;
}
#vertical-menu-wrapper .menu-close{
	color: #fff;
	font-size: 18px;
	margin-top: 0;
	float: right;
	display: block;
	cursor: pointer;
	opacity: 1;
	font-weight: normal;
	text-shadow:none;
	line-height: 1.5;
}

#vertical-menu-wrapper .menu-close:after{
	content: '\e60a';
	font-family: 'icomoon';
	
}
#menu{
	
	position: relative;
	background: #4E4E4E;
	color: #E1E1E1;
	height: auto;
}
#menu ul{
	margin: 0;
	padding: 0;
	height: auto;
	background: #4E4E4E;
	color: #E1E1E1;
}
#menu ul li{
	margin: 0;
	display: block;
	background: #4E4E4E;
	color: #E1E1E1;
}	
#menu ul li i.jstree-icon,#menu ul li i.jstree-ocl,#menu ul li i.jstree-icon:empty{
	display: none;
}
#menu ul li a{
	outline: 0;
	display: inline-block;
	padding: 4px 10px;
	font-size: 16px;
	font-weight: normal;
	width: 100%;
	border-bottom: 1px solid #838383;
	box-shadow: none;
}
	 
#menu ul li:last-child{
	margin-bottom: 10px;
}
#menu ul li a:hover{
	background: #336699;
	color: #fff;
	outline: 0;
	border-radius: 0;
	border-bottom: 1px solid #4E4E4E;
}
#menu ul li.jstree-open a.jstree-clicked
#menu ul li.jstree-open a:hover,
#menu ul li.jstree-open a:focus{
	background: #E3801B;
	color: #E1E1E1;
	outline: 0;
	width: 100%;
	outline: 0;
	border-radius: 0;
	border-bottom: 1px solid #4E4E4E;
}
#menu ul li.jstree-closed > a:after{
	content: "\e602";
	color: #E1E1E1;
	font-size: 16px;
    position: absolute;
    right: 10px;
    font-family: 'icomoon'; 
}

#menu ul li.jstree-open > a:after{
	content: "\e600";
	color: #E1E1E1;
	font-size: 16px;
    position: absolute;
    right: 10px;
    font-family: 'icomoon';
}

#menu ul li.jstree-node, #menu ul li a.jstree-anchor{
	height: auto;
	min-height: auto;
}
 
#menu ul li a:hover,#menu ul li a.jstree-clicked:hover{
	background: #336699;
	color:#E1E1E1;
	outline: 0;
}
#menu ul li a.jstree-clicked{
	background: #4E4E4E;
	color:#E1E1E1;
}
/* --- Sub Menu ---- */

#menu ul li ul{
	margin: 0;
	padding: 0;
	height: auto;
	background: #4E4E4E;
	color: #E1E1E1;
}
#menu ul li ul li{
	margin: 0;
	padding: 0;
	background: #4E4E4E;
	color: #E1E1E1;
}
#menu ul li ul li > a{
	font-size: 14px;
	font-weight: normal;
	padding: 4px 18px;
}
#menu ul li ul li > a:hover{
	border-bottom: 1px solid #4E4E4E;
}
#menu ul li ul li.jstree-closed > a{
	background:#336699;
	color: #E1E1E1;
	border-bottom: 1px solid #4E4E4E;
	border-radius: 0;
}
#menu ul li ul li.jstree-open > a{
	background:#E3801B; 
	color: #E1E1E1;
	border-bottom: 1px solid #4E4E4E;
	border-radius: 0;
}
/*-------------------------------content-wrapper-------------------------------*/
#content-wrapper{
	min-height: 500px;
	padding-top: 30px;
	padding-bottom: 30px;
}
/*---------------------------------Footer----------------------------------*/
/*------- Footer --------*/
footer{
	background: #F2F2F2;
	min-height: 45px;
	max-width: 1110px;
	margin: 0 auto !important;
	bottom: 0;
/* 	position: absolute;
bottom: 0;
left: 0; */
}
#footer-content{
	padding: 0 12px;
	width: 100%;
}
#page-progress{
	width: 100%;
	padding: 0;
}
/*-- Main Page Progress --*/
.progress{
	background: #9C9C9C;
	width: 200px;
	height: 3px;
}
#page-progress{
	margin-bottom: 0;
}
#page-progress .progress-bar{
	background: #1B9ED4;
}
/*-- Audio Progress --*/
#audio-wrapper{
	margin: 4px;
}
/*-- Audio Progress --*/

#footer-content #left-block a,#footer-content #right-block a{
	cursor: pointer;
	display: inline-block;
	text-decoration: none;
} 
/*-- Left block --*/
#footer-content #left-block{
	padding-top:11px;
	float: left;
	width: 67%;
	/*padding-left:12px; */
}
#footer-content #left-block [class*=" icon-"]{
	font-size: 14px;
	color: #4E4E4E;
}
.icon-play{
	font-size: 16px;
	color: #4E4E4E;
}
#footer-content #left-block .icon-block ,#audio-progress{
	margin-right: 12px;
	cursor: pointer;
}
#footer-content #left-block label{
	font-weight: normal;
	font-size: 14px;
	margin-top: -2px;
	max-width: 100%;
	cursor: pointer;
}
/*-- Right Block --*/
#right-block{
	float: left;
	width: 33%;
}

#footer-content #right-block a#click-me{
	color: #E3801B;
	font-style: italic;
	padding-top: 11px;
	margin-left: 42px;
}

#pagination .icon-left-arrow,#pagination .icon-right-arrow{
	font-size: 35px;
	color: #4E4E4E;
	width: 20px;
	display: block;
}

#pagination{
	display: inline-block;
}
#footer-content #right-block .v-divider{
	background: #D7D6D6;
	min-height: 42px;
	width: 1px;
	display: block;
	position: absolute;
	top: 0;
	right: 170px;
}
#pagination i, #pagination div{
	display: block;
	position: absolute;
}
#pagination .icon-left-arrow{
	top:7px;
	right: 136px;
}
#pagination .icon-right-arrow{
	top:7px;
	right: 9px;
}
#pagination #page-num{
	color: #fff;
	background: #E3801B;
	padding: 6px;
	margin: 0 20px;
	top: 0;
	right: 31px;
}
#pagination #page-num label{
	font-size:20px; 
	top:0; 
	float: left;
	line-height: 3rem; 
	font-weight: normal;
}
/*---------------------------------- Media Query ---------------------------------*/
@media screen and (max-width: 1199px) { 
	#footer-content #right-block a{
		margin-left: 0;
	}
	#footer-content #right-block .v-divider{
		right: 162px;
	}
}

/*--------------------- Media Query ---------------------------------*/

@media screen and (max-width: 991px) { 
/*---------------- Header --------------  */
	header{padding: 15px 0;}
	header h1, header h2{ display: block;border: 0;}
	header #title-wrapper{border-right: #cecdcd 1px solid;}
	header #logo-wrapper{margin-top: 20px;}
	header #logo-wrapper span{display: none;}
	header #menu-wrapper{margin-top: 12px;}
/*---------------- Header --------------  */
/*---------------- Footer -----------------*/
	#footer-content #left-block{
		padding-top: 2px;
	}
	#footer-content #left-block .icon-block {
		margin-right: 10px;
	}
	#footer-content #left-block i::after{
		content: "|";
		display: inline-block;
		color: #D7D6D6;
		/*position: absolute;*/
		top: 0;
		font-size: 25px;
		margin-left: 10px;
	}
	#footer-content #right-block .v-divider{
		right: 170px;
	}
 }
 @media screen and (max-width: 767px) { 
/*---------------- Header --------------  */
	header #title-wrapper,header h1, header h2{
		margin-right: 0;
		padding-right: 0;
	}
/*---------------- Header --------------  */

}
 @media screen and (max-width: 430px) {
/*---------------- Header --------------  */
 	header h1{font-size: 16px;}
 	header h2{ font-size: 14px;}
	header #menu-wrapper span{font-size: 21px;}
	header #menu-wrapper{margin-top: 16px;}
/*---------------- Header --------------  */
/*---------------- Footer ---------------*/
	#footer-content #left-block{
		padding-top: 5px;
	}
	#footer-content #left-block .icon-block {
		margin-right: 2px;
	}
	#footer-content #left-block i::after{
		margin-left: 5px;
	}
	
/*---------------- Footer ---------------*/
 }
 @media screen and (max-width: 325px) {
/*---------------- Footer ---------------*/
	#footer-content #right-block .v-divider{
		right: 157px;
	}
	#pagination .icon-left-arrow{
	right: 123px;
	}
	#pagination .icon-right-arrow{
		right: 9px;
	}
	#pagination #page-num{
		right: 25px;
	}
/*---------------- Footer ---------------*/
 }